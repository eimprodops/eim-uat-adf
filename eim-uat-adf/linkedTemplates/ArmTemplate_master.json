{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eim-uat-adf"
		},
		"eim_hana_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eim_hana'"
		},
		"eim_g2lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'eim_g2lake'"
		},
		"eim_sqldb_dyn365byod_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'eim_sqldb_dyn365byod'"
		},
		"eimprodkv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://eim-prod-kv.vault.azure.net/"
		},
		"eim_g2lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eimuatg2lake.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "eim-uat-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"eim_g2lake_accountKey": {
						"value": "[parameters('eim_g2lake_accountKey')]"
					},
					"eim_hana_connectionString": {
						"value": "[parameters('eim_hana_connectionString')]"
					},
					"eim_sqldb_dyn365byod_connectionString": {
						"value": "[parameters('eim_sqldb_dyn365byod_connectionString')]"
					},
					"eimprodkv_properties_typeProperties_baseUrl": {
						"value": "[parameters('eimprodkv_properties_typeProperties_baseUrl')]"
					},
					"eim_g2lake_properties_typeProperties_url": {
						"value": "[parameters('eim_g2lake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}